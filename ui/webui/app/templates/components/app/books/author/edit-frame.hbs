{{#e-form/fields model=params.model
                 register=(action 'registerFields')
                 unregister=(action 'unregisterFields') as |form|}}
  <dl class="dl-horizontal">
    <dt>First Name:</dt>
    <dd>{{form.text value=params.model.firstName required=true}}</dd>

    <dt>Last Name:</dt>
    <dd>{{form.text value=params.model.lastName required=true}}</dd>

    <dt>Middle Name date:</dt>
    <dd>{{form.text value=params.model.middleName}}</dd>
  </dl>
{{/e-form/fields}}