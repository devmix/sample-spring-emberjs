{{#each items as |item index|}}
  {{#if hasBlock}}
    {{yield (hash row=(component 'e-table/row' model=item index=index component=component)) this}}
  {{else}}
    {{component 'e-table/row' model=item index=index component=component}}
  {{/if}}
{{else}}
  {{#if items.isPending}}
    {{e-progress/loading}}
  {{else if items.isRejected}}
    Fail
  {{else}}
    No data
  {{/if}}
{{/each}}
